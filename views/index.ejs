<h1> Url Shortener</h1>
<h3>Shorten your urls completely free, forever...</h3>
<%- include("error-message.ejs") %>
<form action="/" method="POST">
    <div id="url-text">
        <label for="url">Enter your url</label>
        <input type="text" name="url">
    </div>
    <div id="submit">
        <button type="submit">Shorten</button>
    </div>
    
</form>
<%- include("shortened-url-msg.ejs") %>